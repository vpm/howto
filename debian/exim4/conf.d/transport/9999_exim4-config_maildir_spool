maildir_spool:
   debug_print = "T: maildir_spool for $local_part_data@$domain_data"
   driver = appendfile
   directory = /var/spool/mail/$local_part_data/Maildir
   create_directory
   delivery_date_add
   envelope_to_add
   return_path_add
   maildir_format
   directory_mode = 0700
   mode = 0600
   mode_fail_narrower = false
